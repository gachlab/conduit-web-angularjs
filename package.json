{
  "name": "@gachlab/conduit-web-angularjs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "sirv public --host 0.0.0.0",
    "build": "rollup -c",
    "build:docker": "docker compose -f docker-compose.build.yml build ",
    "push:docker":"$DOCKER_REGISTRY_SERVER_PASSWORD | docker login -u $DOCKER_REGISTRY_SERVER_USERNAME --password-stdin $DOCKER_REGISTRY_SERVER_URL && docker compose -f docker-compose.build.yml push",
    "dev": "npm i && rollup -c -w",
    "dev:docker": "docker compose up"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "angular": "^1.8.2",
    "angular-route": "^1.8.2",
    "rollup": "^2.60.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gachlab/conduit-web-angularjs.git"
